{
  "name": "@temporaldynamics/eco-packer",
  "version": "1.1.0",
  "description": "Secure ECO/ECOX packer for Temporal Dynamics pipelines with Timeline Engine support",
  "main": "dist/index.js",
  "module": "dist/index.js",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "require": "./dist/index.js"
    },
    "./eco-utils": {
      "types": "./dist/eco-utils.d.ts",
      "import": "./dist/eco-utils.js",
      "require": "./dist/eco-utils.js"
    }
  },
  "scripts": {
    "clean": "rm -rf dist tsconfig.tsbuildinfo",
    "build": "npm run clean && tsc",
    "dev": "tsc --watch",
    "typecheck": "tsc --noEmit",
    "test": "vitest run",
    "test:watch": "vitest",
    "lint": "eslint src/**/*.ts",
    "prepare": "npm run build"
  },
  "keywords": [
    "eco",
    "eco-pack",
    "signatures",
    "manifest",
    "temporal-dynamics"
  ],
  "author": "Temporal Dynamics LLC",
  "license": "SEE LICENSE IN LICENSE",
  "dependencies": {
    "jsonschema": "^1.5.0",
    "jszip": "^3.10.1",
    "node-forge": "^1.3.1"
  },
  "devDependencies": {
    "@types/jest": "^29.0.0",
    "@types/jsonschema": "^0.0.5",
    "@types/jszip": "^3.4.1",
    "@types/node": "^18.0.0",
    "eslint": "^8.20.0",
    "ts-node": "^10.9.1",
    "typescript": "^4.9.0",
    "vitest": "^3.2.4"
  },
  "files": [
    "dist/**/*"
  ]
}
